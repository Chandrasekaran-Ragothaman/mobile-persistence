<?xml version="1.0" encoding="windows-1252" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="data-synch-tf">
    <default-activity>DataSynch</default-activity>
    <view id="DataSynch">
      <page>/oracle.ateam.sample.mobile.datasynch/DataSynch.amx</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>DataSynch</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>toSelf</from-outcome>
        <to-activity-id>DataSynch</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-mobile-config>
